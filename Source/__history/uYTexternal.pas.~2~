  Vcl.ExtCtrls;

type
  TForm3 = class(TForm)
    pnl1: TPanel;
    wb1: TWebBrowser;
    btn1: TButton;
    btn2: TButton;
    procedure btn2Click(Sender: TObject);
    procedure btn1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form3: TForm3;

